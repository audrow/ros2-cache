{
  "name": "ros2-cache",
  "description": "Cache ROS 2 files",
  "version": "0.0.1",
  "source": "src/main.ts",
  "main": "dist/main.umd.js",
  "module": "dist/main.modern.module.js",
  "unpkg": "dist/main.umd.js",
  "types": "dist/main.d.ts",
  "keywords": [
    "ros2",
    "cache"
  ],
  "files": [
    "dist/**.js*",
    "dist/**/*.ts"
  ],
  "scripts": {
    "build": "microbundle --define PKG_VERSION=$npm_package_version",
    "watch": "microbundle watch --define PKG_VERSION=$npm_package_version",
    "test": "ava",
    "lint": "prettier src test -c",
    "format": "prettier src test --write",
    "prepack": "rm -rf dist && npm run build",
    "release": "npm run prepack && source .env && npx np"
  },
  "devDependencies": {
    "ava": "^4.3.1",
    "microbundle": "^0.15.0",
    "prettier": "^2.6.2"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false
  },
  "license": "MIT",
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "directories": {
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/audrow/ros2-cache.git"
  },
  "author": "Hugo",
  "bugs": {
    "url": "https://github.com/audrow/ros2-cache/issues"
  },
  "homepage": "https://github.com/audrow/ros2-cache#readme"
}
